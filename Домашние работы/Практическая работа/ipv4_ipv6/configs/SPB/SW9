en
 conf t
 no ip domain-lookup
 hostname SW9
 interface Loopback9
  ip address 2.2.2.9 255.255.255.255
  ipv6 address FE80:BB::9 link-local
  ipv6 enable
  exit
 vlan 10
  vlan 20
  vlan 99
  exit
 interface range ethernet 0/0-1
  switchport trunk allowed vlan 10,20,99
  switchport trunk encapsulation dot1q
  switchport mode trunk
  channel-group 1 mode on
  exit
 interface Ethernet0/2
  switchport access vlan 10
  switchport mode access
  exit
 interface Ethernet1/0
  switchport trunk allowed vlan 10,20
  switchport trunk encapsulation dot1q
  switchport mode trunk
  exit
 interface Vlan 99
  ip address 1.1.20.9 255.255.255.0
  ipv6 address FE80:BA::9 link-local
  ipv6 address 2002:ABCD:0DB8:99::9/64
  ipv6 enable
  exit
 ip default-gateway 1.1.20.1
 do wr
