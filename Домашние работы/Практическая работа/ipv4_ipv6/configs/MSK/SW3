en
conf t
no ip domain-lookup
hostname SW3
vlan 10
 vlan 20
 vlan 99
 exit
interface Loopback3
 ip address 1.1.1.3 255.255.255.255
 ipv6 address FE80:AA::3 link-local
 ipv6 enable
 exit
interface Ethernet0/0
 switchport trunk allowed vlan 10,20,99
 switchport trunk encapsulation dot1q
 switchport mode trunk
 no shutdown
 exit
interface Ethernet0/1
 switchport trunk allowed vlan 10,20,99
 switchport trunk encapsulation dot1q
 switchport mode trunk
 no shutdown
 exit
interface Ethernet0/2
 switchport access vlan 10
 switchport mode access
 no shutdown
 exit
interface Vlan99
 ip address 1.1.10.3 255.255.255.0
 ipv6 address FE80:AB::3 link-local
 ipv6 address 2022:ABCD:DB8:A::3/64
 ipv6 enable
 no shutdown
 exit
ip default-gateway 1.1.10.1
do wr
