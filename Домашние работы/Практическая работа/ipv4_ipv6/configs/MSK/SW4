conf t
no ip domain-lookup
hostname SW4
vlan 10
 vlan 20
 vlan 99
 exit
interface Loopback4
 ip address 1.1.1.4 255.255.255.255
 ipv6 address FE80:AA::4 link-local
 ipv6 enable
 exit
interface Port-channel1
 switchport trunk allowed vlan 10,20,99
 switchport trunk encapsulation dot1q
 switchport mode trunk
 exit
interface Ethernet0/0
 switchport trunk allowed vlan 10,20,99
 switchport trunk encapsulation dot1q
 switchport mode trunk
 no shutdown
 exit
interface Ethernet0/1
 switchport trunk allowed vlan 10,20,99
 switchport trunk encapsulation dot1q
 switchport mode trunk
 no shutdown
 exit
interface Ethernet0/2
 switchport trunk allowed vlan 10,20,99
 switchport trunk encapsulation dot1q
 switchport mode trunk
 channel-protocol lacp
 channel-group 1 mode active
 no shutdown
 exit
interface Ethernet0/3
 switchport trunk allowed vlan 10,20,99
 switchport trunk encapsulation dot1q
 switchport mode trunk
 channel-protocol lacp
 channel-group 1 mode active
 no shutdown
 exit
interface Ethernet1/0
 switchport trunk allowed vlan 10,20
 switchport trunk encapsulation dot1q
 switchport mode trunk
 no shutdown
 exit
interface Vlan99
 ip address 1.1.10.4 255.255.255.0
 ipv6 address FE80:AB::4 link-local
 ipv6 address 2022:ABCD:DB8:A::4/64
 ipv6 enable
 no shutdown
 exit
ip default-gateway 1.1.10.1
do wr
